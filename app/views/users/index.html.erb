<div class="wrapper">
	<div class="box">
		<div class="row row-offcanvas row-offcanvas-left">
			<div class="column col-sm-12 col-xs-11" id="main">
				<div class="padding">
					<div class="full col-sm-9">
						<div class="row">
							<div class="col-sm-7">
								<% @users.each do |user| %>
								<div class="panel panel-default">
									<li class="round-image-50 dropdown"><%= image_tag(user.avatar.url(:thumb)) %></li>
									<div class="panel-heading"><h6><%= user.username %></h6></div>
									<div class="panel-heading"><p><%= user.about %></p></div>
								</div>
								<% end %>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>